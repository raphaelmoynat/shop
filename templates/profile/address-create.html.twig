{% extends 'base.html.twig' %}



{% block body %}
    {{ form_start(form) }}

    <div class="mb-3">
        {{ form_row(form.streetNumber)}}
    </div>
    <div class="mb-3">
        {{ form_row(form.street)}}
    </div>
    <div class="mb-3">
        {{ form_row(form.zipcode)}}
    </div>
    <div class="mb-3">
        {{ form_row(form.city)}}
    </div>
    <div class="mb-3">
        {{ form_row(form.phone)}}
    </div>
    <div class="mb-3">
        {{ form_row(form.country)}}
    </div>

    <button type="submit" class="btn btn-success">{{ btnValue }}</button>

    {{ form_end(form) }}
{% endblock %}